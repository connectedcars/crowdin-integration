{
  "name": "crowdin-integration",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-typescript": "7.13.0",
    "@connectedcars/setup": "^0.15.0",
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "babel-jest": "26.6.3",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "26.6.3",
    "prettier": "2.5.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@crowdin/crowdin-api-client": "^1.12.3",
    "@types/nconf": "^0.10.1",
    "axios": "0.21.3",
    "nconf": "^0.11.3"
  },
  "bin": {
    "fetchTranslations": "build/dist/bin/fetchTranslations.js",
    "uploadTranslations": "build/dist/bin/uploadTranslations.js"
  },
  "scripts": {
    "test": "TZ=UTC jest",
    "cov": "TZ=UTC jest --coverage=true",
    "build": "tsc -p .",
    "build:js": "build --skip-typings src",
    "build:types": "tsc --noEmit",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "ci-jest": "TZ=UTC jest --silent --no-color --json 2> /dev/null; res=$?; echo; exit $res",
    "ci-audit": "npm audit --json || true",
    "ci-eslint": "eslint --format json './src/**/*.{ts,tsx}' || true",
    "prepare": "npm run build",
    "prepack": "npm run build",
    "prepublishOnly": "rm -rf build"
  },
  "engines": {
    "node": ">=10.15.0"
  }
}
